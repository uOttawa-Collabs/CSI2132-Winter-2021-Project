<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <listener>
        <listener-class>
            team.returnteamname.myhotelcustomer.eventlistener.ServerStartStopListener
        </listener-class>
    </listener>
    <servlet>
        <servlet-name>
            BookServlet
        </servlet-name>
        <servlet-class>
            team.returnteamname.myhotelcustomer.servlet.BookServlet
        </servlet-class>
    </servlet>
    <servlet>
        <servlet-name>
            LoginServlet
        </servlet-name>
        <servlet-class>
            team.returnteamname.myhotelcustomer.servlet.LoginServlet
        </servlet-class>
    </servlet>
    <servlet>
        <servlet-name>
            QueryServlet
        </servlet-name>
        <servlet-class>
            team.returnteamname.myhotelcustomer.servlet.QueryServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BookServlet</servlet-name>
        <url-pattern>/BookServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryServlet</servlet-name>
        <url-pattern>/QueryServlet</url-pattern>
    </servlet-mapping>
</web-app>
